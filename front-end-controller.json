{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"frontend",
    "labels":{
      "name":"frontend"
    }
  },
  "spec":{
    "replicas":3,
    "selector":{
      "name":"frontend"
    },
    "template":{
      "metadata":{
        "labels":{
          "name":"frontend"
        }
      },
      "spec":{
        "containers":[
          {
            "name":"keystone",
            "image":"chrisgornall/d29blog:latest",
            "ports":[
              {
                "containerPort":3000,
                "protocol":"TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
